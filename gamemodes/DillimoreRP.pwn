CMD:ori(playerid,params[])
{
    LociraoVozilo[playerid] = 0;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
	else if(PlayerInfo[ playerid ][ pPosao ] != 3) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi seljak!");
	else if(!IsTrailerAttachedToVehicle(GetPlayerVehicleID(playerid))) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi prikvacio prikljucak!");
	else if(Polje[playerid]>0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Vec imas pokrenutu radnju!");
	else if(GetPlayerVehicleID(playerid) != SeljakVozila[2] && GetPlayerVehicleID(playerid) != SeljakVozila[4]) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti na traktoru!");
	else if(Polje1 == 0 && Polje2 > 0)
	{
		SendClientMessage(playerid,-1,""PLAVA"[Oranje]"BIJELA" Pokrenuo si oranje polja");
		Polje[playerid] = 1;
		PoljeStanica[playerid] = 1;
		SetPlayerCheckpoint(playerid,847.9203,-257.0427,21.7610,5.0);
	}
	
	else if(Polje1 > 0 && Polje2 == 0)
	{
		SendClientMessage(playerid,-1,""PLAVA"[Oranje]"BIJELA" Pokrenuo si oranje polja");
		Polje[playerid] = 2;
		PoljeStanica[playerid] = 1;
		SetPlayerCheckpoint(playerid,1000.1522,-408.9475,67.1189,5.0);
	}
	
	else if(Polje1 == 0 && Polje2 == 0)
 	{
		ShowPlayerDialog(playerid,DIALOG_POLJE,DIALOG_STYLE_LIST,"Odabir polja","Polje 1\nPolje 2","U redu","Odustani");
	}
	
	else if(Polje1 > 0 && Polje2 > 0)
	{
		SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Oba polja su vec uzorana");
	}
	return 1;
}

