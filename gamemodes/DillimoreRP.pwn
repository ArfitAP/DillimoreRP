    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
	else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else
	{
		ShowPlayerDialog(playerid,DIALOG_PRODAJA,DIALOG_STYLE_LIST,""NARANCASTA"Prodaja imovine",""BIJELA"Bicikl\nMotor\nAuto\nStan\nKuca\nFirma","Odaberi","Odustani");
	}
	return 1;
}

CMD:zamjena(playerid,params[])
{
	new id,mijenja,treba,doplata,Float:x,Float:y,Float:z,Mijenja[30],Treba[30];
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else if(sscanf(params,"uiii",id,mijenja,treba,doplata))
	{
		SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /zamjena [id / name] [STO MIJENJATE] [ZA STO MIJENJATE] [DOPLATA KOJU VI DOBIVATE]");
		SendClientMessage(playerid,-1,"Dostupan odabir zamjene: 1-bicikl, 2-motor, 3-auto, 4-stan, 5-kuca, 6-firma");
	}
	else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan id korisnika");
	else if(id == playerid) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne mozes sam sebi ponuditi zamjenu");
	else if(mijenja < 1 || mijenja > 6 || treba < 1 || treba > 6) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan odabir");
	else if(doplata < 1 || doplata > 5000000) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Doplata mora biti izmedu 1 i 5,000,000 $");
	else
	{
	    GetPlayerPos(id,x,y,z);
	    if(!IsPlayerInRangeOfPoint(playerid,10.0,x,y,z)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac nije u vasoj blizini");
		else if(mijenja == 1 && PlayerInfo[playerid][pImaBicikl] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne posjedujes bicikl");
        else if(mijenja == 2 && PlayerInfo[playerid][pImaMotor] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne posjedujes motor");
        else if(mijenja == 3 && PlayerInfo[playerid][pImaVozilo] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne posjedujes auto");
        else if(mijenja == 4 && PlayerInfo[playerid][pImaStan] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne posjedujes stan");
        else if(mijenja == 5 && PlayerInfo[playerid][pImaKucu] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne posjedujes kucu");
        else if(mijenja == 6 && PlayerInfo[playerid][pImaFirmu] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne posjedujes firmu");
        else if(treba == 1 && PlayerInfo[id][pImaBicikl] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac ne posjeduje bicikl");
        else if(treba == 2 && PlayerInfo[id][pImaMotor] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac ne posjeduje motor");
        else if(treba == 3 && PlayerInfo[id][pImaVozilo] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac ne posjeduje auto");
        else if(treba == 4 && PlayerInfo[id][pImaStan] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac ne posjeduje stan");
        else if(treba == 5 && PlayerInfo[id][pImaKucu] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac ne posjeduje kucu");
        else if(treba == 6 && PlayerInfo[id][pImaFirmu] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac ne posjeduje firmu");
        else
        {
            PonudioZamjenu[id] = playerid;
            PonudioZamjenuIgracu[playerid] = id;
            NudiUZamjeni[playerid] = mijenja;
            TraziUZamjeni[playerid] = treba;
            TraziDoplate[playerid] = doplata;

            switch(mijenja)
            {
                case 1: Mijenja = "Bicikl";
                case 2: Mijenja = "Motor";
                case 3: Mijenja = "Auto";
                case 4: Mijenja = "Stan";
                case 5: Mijenja = "Kucu";
                case 6: Mijenja = "Firmu";
            }
            switch(treba)
            {
                case 1: Treba = "Bicikl";
                case 2: Treba = "Motor";
                case 3: Treba = "Auto";
                case 4: Treba = "Stan";
                case 5: Treba = "Kucu";
                case 6: Treba = "Firmu";
            }
            
            new msg[80],namex[32];
			GetPlayerName(playerid,namex,sizeof(namex));
			format(msg,sizeof(msg),""LJUBICASTA" %s vam je punudio %s za vas %s uz vasu doplatu od %d $",namex,Mijenja,Treba,doplata);
			SCM(id,-1,msg);
			GetPlayerName(id,namex,sizeof(namex));
			format(msg,sizeof(msg),""LJUBICASTA" Punudio si %s zamjenu, %s za vas %s uz njegovu doplatu od %d $",namex,Treba,Mijenja,doplata);
			SCM(playerid,-1,msg);
			
			SetTimerEx("Zamjena",60000,false,"d",playerid);
		}
	}
	return 1;
}

CMD:dajvozilo(playerid,params[])
{
	new id;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[playerid][pAdmin] == 0) SendClientMessage(playerid,-1,""CRVENA"Samo admini");
    else if(PlayerInfo[playerid][pAdmin] < 2) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti minimalno admin lvl 2 da bi koristio ovu komandu");
    else if(sscanf(params,"u",id)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /dajvozilo [id / name]");
    else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan ID korisnika");
    else
    {
        new i;
        for(i=1;i<MAX_VOZILA;i++)
        {
            if(IsPlayerInVehicle(playerid,VoziloInfo[i][vID]))
            {
                if(VoziloInfo[i][vImaVlasnika] == 1) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ovo vozilo ima vlasnika");
                else if(PlayerInfo[id][pImaVozilo]) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac vec posjeduje vozilo");
                else
                {
                    new namex[32],msg[75];
					GetPlayerName(playerid,namex,sizeof(namex));
					PlayerInfo[id][pVozilo] = i;
                    PlayerInfo[id][pImaVozilo] = 1;
                    VoziloInfo[i][vImaVlasnika] = 1;
                    strmid(VoziloInfo[i][vVlasnik], namex, 0, strlen(namex), 999);
					SacuvajVozilo(i);
     		  	    format(msg,sizeof(msg),""LJUBICASTA"Admin %s vam je dodijelio vozilo, /v za vise informacija",namex);
					SCM(id,-1,msg);
					GetPlayerName(id,namex,sizeof(namex));
					format(msg,sizeof(msg),""LJUBICASTA"Dodijelili ste %s vozilo",namex);
					return SCM(playerid,-1,msg);
				}
			}
		}
		SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti u prodajnom vozilu");
	}
	return 1;
}
