{
	new vozilo = vehicleid,i,Float:x,Float:z,Float:y;
	GetPlayerPos(playerid,x,y,z);
	KillTimer(TimerPovlacenje[playerid]);
	TimerPovlacenje[playerid] = 0;
    if(SmetlarRuta[playerid]>0 && vozilo != SmetlarKamion[0] && vozilo != SmetlarKamion[1])
    {
		SendClientMessage(playerid,-1,""CRVENA"Ne smijete napustati smetlarsko vozilo!");
		SendClientMessage(playerid,-1,""CRVENA"Zbog neobavljenog posla platit cete opomenu u iznosu 200$");
		GivePlayerMoney(playerid,-200);
		SmetlarRuta[playerid] = 0;
		SmetlarStanica[playerid] = 0;
		DisablePlayerCheckpoint(playerid);
		KillTimer(TimerSmetlar[playerid]);
		TimerSmetlar[playerid] = 0;
	}

	else if(Mlijeko[playerid] == 1 || MlijekoProdaja[playerid] == 1)
	{
	    if(MlijekoProdaja[playerid] == 1 && (vozilo == SeljakVozila[0] || vozilo == SeljakVozila[1])) SendClientMessage(playerid,-1,""PLAVA"[Dojenje]"BIJELA" Odvezi mlijeko do mljekare");
