	else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Pogresan ID korisnika");
	else if(kolicina<1 || (PlayerInfo[playerid][pAdmin] < 3 && kolicina>5000)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Pogresna kolicina novca, maximalno mozes dati 5000 $");
