public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
    if((newkeys & KEY_YES) && !(oldkeys & KEY_YES))
    {
        if(IsPlayerInAnyVehicle(playerid) && GetPlayerVehicleSeat(playerid) == 0 && toci[playerid] == 0)
        {
            new msg[150],namex[32],vozilo = GetPlayerVehicleID(playerid);
            if(!VoziloJeBicikl(vozilo))
            {
                new engine, lights, alarm, doors, bonnet, boot, objective;
				GetVehicleParamsEx(vozilo, engine, lights, alarm, doors, bonnet, boot, objective);
	            if(engine == 0)
	            {
	                new i;
	                for(i=1;i<MAX_VOZILA+1;i++)
					{
					    if(i == MAX_VOZILA) break;
					    if(vozilo == VoziloInfo[i][vID])
					    {
					        if(VoziloInfo[i][vImaVlasnika] == 0) return SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne mozes upaliti prodajno vozilo");
					        else
					        {
					            if(PlayerInfo[playerid][pVozilo] != i  && PlayerInfo[playerid][pAdmin] == 0) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemas kljuc da bi upalio motor vozila");
				                else if(Gorivo[vozilo] == 0.2) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nema goriva");
				                else
				                {
					                SetVehicleParamsEx(vozilo, 1, lights, alarm, doors, bonnet, boot, objective);
					                TimerGorivo[playerid] = SetTimerEx("PotrosnjaGoriva",15000,true,"d",playerid);
					                GetPlayerName(playerid,namex,sizeof(namex));
					                format(msg,sizeof(msg),""SIVA" *%s pali motor vozila",namex);
