forward OcistiVarijable(playerid);
public OcistiVarijable(playerid)
{
    Spec[playerid] = -1;

	if(AdminVozilo[playerid] > 0)
	{
	    new engine, lights, alarm, doors, bonnet, boot, objective;
		GetVehicleParamsEx(AdminVozilo[playerid], engine, lights, alarm, doors, bonnet, boot, objective);
		SetVehicleParamsEx(AdminVozilo[playerid], 0, lights, alarm, doors, bonnet, boot, objective);
 		DestroyVehicle(AdminVozilo[playerid]);
		AdminVozilo[playerid] = -1;
	}
	if(Zapreka[playerid] > 0) DestroyDynamicObject(Zapreka[playerid]);
	if(Radara[playerid] > 0)
	{
		DestroyObject(Radara[playerid]);
		Radar = 0;
		PostavioRadar = -1;
	}
	if(postavioradio[playerid] > -1)
	{
		DestroyObject(postavioradio[playerid]);
		postavioradio[playerid] = -2;
	}
	if(RentaBicikl[playerid] > -1)
	{
		SetVehicleToRespawn(BiciklRent[RentaBicikl[playerid]]);
		RentaBicikl[playerid] = -1;
	}

	StopAudioStreamForPlayer(playerid);

	if(ecp[playerid] == 0) {KillTimer(TimerSkin[playerid]); TimerSkin[playerid] = 0;}
	if(Vuce[playerid] > -1) {KillTimer(TimerPovlacenje[playerid]); TimerPovlacenje[playerid] = 0;}
	if(IsPlayerInAnyVehicle(playerid)) {KillTimer(TimerGorivo[playerid]); TimerGorivo[playerid] = 0;}
	if(SmetlarStanica[playerid] > 0) {KillTimer(TimerSmetlar[playerid]); TimerSmetlar[playerid] = 0;}

	if(PolaganjeStanica[playerid] > 0)
 	{
 	    new engine, lights, alarm, doors, bonnet, boot, objective;
		GetVehicleParamsEx(GetPlayerVehicleID(playerid), engine, lights, alarm, doors, bonnet, boot, objective);
		SetVehicleParamsEx(GetPlayerVehicleID(playerid), 0, lights, alarm, doors, bonnet, boot, objective);
		Gorivo[GetPlayerVehicleID(playerid)] = 40.0;
		SetVehicleToRespawn(GetPlayerVehicleID(playerid));
	 	KillTimer(TimerSkola[playerid]);
		KillTimer(TimerPolaganje[playerid]);
		TimerSkola[playerid] = 0;
		TimerPolaganje[playerid] = 0;
	}

	if(PozvaoMehanicara == playerid) PozvaoMehanicara = -1;
	if(PozvaoHitnu == playerid) PozvaoHitnu = -1;

	if(RazgovaraNaMobitel[playerid] > -1)
