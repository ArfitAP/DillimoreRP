CMD:mute(playerid,params[])
{
	new id,razlog[80],minute;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[playerid][pAdmin] == 0) SendClientMessage(playerid,-1,""CRVENA"Samo admini");
    else if(sscanf(params,"uis[80]",id,minute,razlog)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /mute [id / name] [vrijeme u minutama] [razlog]");
    else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Pogresan ID korisnika");
    else if(id == playerid) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Ne mozes sam sebe mutirati");
    else if(PlayerInfo[id][pMutiran] == 1) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Taj igrac je vec mutiran, koristi /unmute");
    else if(minute < 1 || minute > 60) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Vrijeme mora biti izmedu 1 i 60 minuta");
    else if(PlayerInfo[id][pAdmin] > 0 || PlayerInfo[id][pGS] > 0) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Nemozes mutati Admina ili GS-a!");
    else
    {
        new msg[150],namex[32],namex1[32];
        GetPlayerName(playerid,namex,sizeof(namex));
        format(msg,sizeof(msg),""CRVENA"Admin %s vam je zabranio pisanje sljedecih %d sekundi zbog %s",namex,minute*60,razlog);
        SCM(id,-1,msg);
        GetPlayerName(id,namex1,sizeof(namex1));
        format(msg,sizeof(msg),""CRVENA"Zabranio si %s pisanje sljedecih %d sekundi zbog %s",namex1,minute*60,razlog);
        SCM(playerid,-1,msg);
        format(msg,sizeof(msg),""CRVENA"[Mute]"BIJELA" Admin %s je mutirao igraca %s zbog %s",namex,namex1,razlog);
		foreach(Player,i)
		{
			if(i != playerid && PlayerInfo[i][pAdmin] > 0) SCM(i,-1,msg);
		}
        PlayerInfo[id][pMutiran] = 1;
        PlayerInfo[id][pMutiranVrijeme] = minute*60;
        TimerMute[id] = SetTimerEx("Mute",5000,true,"d",id);
    }
    return 1;
}

