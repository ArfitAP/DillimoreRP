CMD:kick(playerid,params[])
{
	new id,razlog[80];
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[playerid][pAdmin] == 0) SendClientMessage(playerid,-1,""CRVENA"Samo admini");
    else if(sscanf(params,"us[80]",id,razlog)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /kick [id / name] [razlog]");
    else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Pogresan ID korisnika");
    else if(PlayerInfo[id][pAdmin] > 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne mozes admina izbaciti iz igre!");
    else if(id == playerid) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Ne mozes sam sebe kickati");
    else
    {
        new msg[150],namex[32],namex1[32];
        GetPlayerName(playerid,namex,sizeof(namex));
        format(msg,sizeof(msg),""CRVENA"[Kick] Izbaceni ste iz igre od strane admina %s zbog %s",namex,razlog);
        SCM(id,-1,msg);
        GetPlayerName(id,namex1,sizeof(namex1));
        format(msg,sizeof(msg),""CRVENA"[Kick] Izbacio si %s iz igre zbog %s",namex1,razlog);
        SCM(playerid,-1,msg);
        format(msg,sizeof(msg),""CRVENA"[Kick]"BIJELA" Admin %s je izbacio igraca %s iz igre zbog %s",namex,namex1,razlog);
		foreach(Player,i)
		{
			if(i != playerid && PlayerInfo[i][pAdmin] > 0) SCM(i,-1,msg);
		}
		SetTimerEx("Kik",750,false,"d",id);
	}
	return 1;
}

CMD:odg(playerid,params[])
{
