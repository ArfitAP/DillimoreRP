CMD:avozilo(playerid,params[])
{
	new model;
	if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
 	else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else if(PlayerInfo[playerid][pAdmin] == 0) SendClientMessage(playerid,-1,""CRVENA"Samo Admini !!");
	else
	{
	    if(AdminVozilo[playerid] > 0)
	    {
			SCM(playerid,-1," Unistio si admin vozilo");
			new engine, lights, alarm, doors, bonnet, boot, objective;
			GetVehicleParamsEx(AdminVozilo[playerid], engine, lights, alarm, doors, bonnet, boot, objective);
			SetVehicleParamsEx(AdminVozilo[playerid], 0, lights, alarm, doors, bonnet, boot, objective);
			DestroyVehicle(AdminVozilo[playerid]);
			AdminVozilo[playerid] = -1;
		}
		else if(IsPlayerInAnyVehicle(playerid))
		{
			if(AVozila(GetPlayerVehicleID(playerid)))
			{
				foreach(Player,i)
				{
				    if(AdminVozilo[i] == GetPlayerVehicleID(playerid))
				    {
						SCM(playerid,-1," Unistio si admin vozilo");
						SCM(i,-1," Unisteno vam je admin vozilo");
						new engine, lights, alarm, doors, bonnet, boot, objective;
						GetVehicleParamsEx(AdminVozilo[i], engine, lights, alarm, doors, bonnet, boot, objective);
						SetVehicleParamsEx(AdminVozilo[i], 0, lights, alarm, doors, bonnet, boot, objective);
						DestroyVehicle(AdminVozilo[i]);
						AdminVozilo[i] = -1;
					}
				}
			}
			else SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Izadi iz vozila da bi stvorio admin vozilo.");
		}
		else
		{
		    if(sscanf(params,"d",model))
			{
				SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /avozilo [model] !");
				SendClientMessage(playerid,-1,"Dostupni modeli: 1-Landstalker, 2-Bravura, 3-Perenniel, 4-Sentinel, 5-Manana ");
				SendClientMessage(playerid,-1,"Dostupni modeli: 6-Washington, 7-Bobcat, 8-Premier, 9-Previon, 10-Admiral ");
				SendClientMessage(playerid,-1,"Dostupni modeli: 11-Solair, 12-Regina, 13-Comet, 14-Virgo, 15-Greenwood ");
		        SendClientMessage(playerid,-1,"Dostupni modeli: 16-Elegant, 17-Nebula, 18-Majestic, 19-Cadrona, 20-Windsor");
		        SendClientMessage(playerid,-1,"Dostupni modeli: 21-Sultan, 22-Bulet, 23-Super GT, 24-Turismo, 25-Infernus");
		  	}
			else if(model<1 || model>25) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan model");
			else if(Avoz == 0) SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Pricekaj 30 sekundi, netko je upravo stvorio vozilo");
			else
			{
			    new Float:x,Float:z,Float:y,Float:a;
				switch(model)
				{
					case 1: model = 400;
					case 2: model = 401;
					case 3: model = 404;
					case 4: model = 405;
					case 5: model = 410;
					case 6: model = 421;
					case 7: model = 422;
					case 8: model = 426;
					case 9: model = 436;
					case 10: model = 445;
					case 11: model = 458;
					case 12: model = 479;
					case 13: model = 480;
					case 14: model = 491;
					case 15: model = 492;
					case 16: model = 507;
					case 17: model = 516;
					case 18: model = 517;
					case 19: model = 527;
					case 20: model = 555;
					case 21: model = 560;
					case 22: model = 541;
					case 23: model = 506;
					case 24: model = 451;
					case 25: model = 411;
				}
				GetPlayerPos(playerid,x,y,z);
				GetPlayerFacingAngle(playerid,a);
				SCM(playerid,-1," Stvorio si admin vozilo");
				Avoz = 0;
				SetTimer("AV",30000,false);
				AdminVozilo[playerid] = CreateVehicle(model,x+2,y+1,z,a,random(255),random(255),-1);
			}
		}
	}
	return 1;
}

CMD:rr(playerid,params[])
{
	new odabir,namex[32],msg[60];
	if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
 	else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else if(PlayerInfo[playerid][pAdmin] == 0) SendClientMessage(playerid,-1,""CRVENA"Samo Admini !!");
 	if(sscanf(params,"d",odabir))
	{
	 	SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /rr [odabir]");
	 	SCM(playerid,-1,"Dostupan odabir: 1-sva vozila, 2-vozila bez prikljucaka seljaka");
	}
	else if(odabir < 1 || odabir > 2) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan odabir");
	else if(odabir == 1)
	{
	    new unwanted[1000];
		foreach(Player,i)
		{
			if(IsPlayerInAnyVehicle(i))  unwanted[GetPlayerVehicleID(i)] = 1;
		}
		for(new car = 1; car < 1000; car++)
		{
			if(!unwanted[car]) SetVehicleToRespawn(car);
		}
		GetPlayerName(playerid,namex,sizeof(namex));
		format(msg,sizeof(msg),""ATEXT" Admin %s je respawnao sva vozila",namex);
		SendClientMessageToAll(-1,msg);
	}
	else if(odabir == 2)
	{
	    new unwanted[1000];
		foreach(Player,i)
		{
			if(IsPlayerInAnyVehicle(i))  unwanted[GetPlayerVehicleID(i)] = 1;
		}
		for(new car = 1; car < 1000; car++)
		{
			if(!unwanted[car] && car != SeljakPrikljucak[0] && car != SeljakPrikljucak[1]) SetVehicleToRespawn(car);
		}
		GetPlayerName(playerid,namex,sizeof(namex));
		format(msg,sizeof(msg),""ATEXT" Admin %s je respawnao vozila",namex);
		SendClientMessageToAll(-1,msg);
	}
	return 1;
}

