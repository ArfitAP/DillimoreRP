public OnPlayerPickUpDynamicPickup(playerid, pickupid)
{
	new i;
	if(pickupid == PosaoPickUp[0]) GameTextForPlayer(playerid, "~g~SMETLAR~n~~w~PISI /posao DA SE ZAPOSLIS KAO SMETLAR",2500,3);
	else if(pickupid == PosaoPickUp[1]) GameTextForPlayer(playerid, "~g~PRODAVAC PIZZE~n~~w~PISI /posao DA SE ZAPOSLIS KAO PRODAVAC PIZZE",2500,3);
	else if(pickupid == PosaoPickUp[2]) GameTextForPlayer(playerid, "~g~SELJAK~n~~w~PISI /posao DA SE ZAPOSLIS KAO SELJAK",2500,3);
	else if(pickupid == PosaoPickUp[3]) GameTextForPlayer(playerid, "~g~MEHANICAR~n~~w~PISI /posao DA SE ZAPOSLIS KAO MEHANICAR",2500,3);
	else if(pickupid == PosaoPickUp[4]) GameTextForPlayer(playerid, "~g~DILER DROGE~n~~w~PISI /posao DA SE ZAPOSLIS KAO DILER DROGE",2500,3);
	else if(pickupid == ProizvodnjaDroge)
	{
	    if(PlayerInfo[playerid][pPosao] == 5)
	    {
	        if(Urod[playerid] > 0)
	        {
			    new msg[150];
			    format(msg,sizeof(msg),""PLAVA"[Droga]"BIJELA" Donio si %d kg uroda u zamjenu za isto toliko grama droge",Urod[playerid]);
			    PlayerInfo[playerid][pDroga] += Urod[playerid];
			    Urod[playerid] = 0;
				SendClientMessage(playerid,-1,msg);
			}
		}
	}

	else if(pickupid == PDOprema && GunTimer[playerid] == 1)
	{
	    GunTimer[playerid] = 0;
		ShowPlayerDialog(playerid,DIALOG_PD_OPREMA,DIALOG_STYLE_LIST,""BIJELA"Oprema",""BIJELA"Patrolna oprema\nOprema za potjeru\nProfesionalna oprema\nSpecijalna oprema\nHealth i Armor","Odaberi","Odustani");
	}

	for(i=1;i<1000;i++)
	{
		if(pickupid == KucaUlazPickUp[i]) 
		{
				if(KucaInfo[i][kImaVlasnika] == 0)
				{
					 new msg[150];
					 format(msg,sizeof(msg),""BIJELA"Kuca na prodaju\nCijena:"ZELENA" %d$\n \n"BIJELA"Za kupnju pritisnite "ZELENA"Kupi"BIJELA" !!!",KucaInfo[i][kVrijednost]);
					 ShowPlayerDialog(playerid,DIALOG_KUCA_KUPNJA,DIALOG_STYLE_MSGBOX,""ZELENA"Kuca kupnja",msg,"Kupi","Odustani");
				}
		}
		
		else if(pickupid == StanUlazPickUp[i])
		{
				if(StanInfo[i][sImaVlasnika] == 0)
				{
					 new msg[150];
					 format(msg,sizeof(msg),""BIJELA"Stan na prodaju\nCijena:"ZUTA" %d$\n \n"BIJELA"Za kupnju pritisnite "ZUTA"Kupi"BIJELA" !!!",StanInfo[i][sVrijednost]);
					 ShowPlayerDialog(playerid,DIALOG_STAN_KUPNJA,DIALOG_STYLE_MSGBOX,""ZUTA"Stan kupnja",msg,"Kupi","Odustani");
				}
		}
		
		else if(pickupid == GunShopPickUp[i] && GunTimer[playerid] == 1)
		{
		    GunTimer[playerid] = 0;
            ShowPlayerDialog(playerid,DIALOG_GUN_SHOP,DIALOG_STYLE_LIST,""PLAVA"Oruzje",""BIJELA"Noz (50 $)\nPistol 9mm (200 $)\nMP5 (300 $)\nCountry Rifle (350 $)\nAk 47(400 $)\nM4 (400 $)\nSniper Rifle (550 $)","Odaberi","Odustani");
		}
	}
	return 1;
}

