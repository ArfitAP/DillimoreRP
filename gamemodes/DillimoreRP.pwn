		format(msg1, sizeof(msg1), ""ZUTA" *(Mobitel) %s kaze: %s", namex, text);
		format(msg2, sizeof(msg2), ""BIJELA" *(Mobitel) %s kaze: %s", namex, text);
		SetPlayerChatBubble(playerid, text, COLOR_WHITE, 50.0, 5000);
		ProxDetector(20.0, playerid,msg2,-1,-1,-1,-1,-1);
		SCM(RazgovaraNaMobitel[playerid],-1,msg1);
	}
	else
	{
		new msg[200],namex[32];
		GetPlayerName(playerid,namex,sizeof(namex));
		format(msg, sizeof(msg), ""BIJELA" *%s kaze: %s", namex, text);
		SetPlayerChatBubble(playerid, text, COLOR_WHITE, 50.0, 5000);
		ProxDetector(20.0, playerid,msg,-1,-1,-1,-1,-1);
	}
	return 0;
}


