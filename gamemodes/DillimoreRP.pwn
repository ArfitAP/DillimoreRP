forward Pay(playerid);
public Pay(playerid)
{
    new msg1[30],sat,minuta,sekunda;
	gettime(sat,minuta,sekunda);
	format(msg1,sizeof(msg1),""CPLAVA"Sada je %d:00h",sat);
	SCM(playerid,-1,msg1);
	if(PlayerInfo[playerid][pPayDayPoeni] < 30) SCM(playerid,-1,""CRVENA"Nisi igrao dovoljno dugo za isplatu place");
	else if(LogedIn[playerid]==1)
	{
	    PlayerInfo[playerid][pSatiIgre]++;
		if(PlayerInfo[playerid][pPosao] > 0) PlayerInfo[playerid][pPosaoUgovor]++;
		if(PlayerInfo[playerid][pClan] > 0) PlayerInfo[playerid][pOrgUgovor]++;
		PlayerInfo[playerid][pRespekti]++;
		PlayerInfo[playerid][pPayDayPoeni] = 0;
		if(PlayerInfo[playerid][pRespekti] == PlayerInfo[playerid][pPotrebnoDoLvlUp])
		{
            PlayerInfo[playerid][pPotrebnoDoLvlUp] +=2;
            PlayerInfo[playerid][pRespekti] = 0;
            PlayerInfo[playerid][pLevel]++;
            SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
            new msg[150];
            format(msg,sizeof(msg),""ZELENA"Cestitamo, presli ste na novi level, vas level je sada %d",PlayerInfo[playerid][pLevel]);
            SCM(playerid,-1,msg);
		}
		new rezije=0,porez=0,struja=0,voda=0,TV=0,plin=0,dodatak=0,placa=0,trosak=0,prihod=0,zarada;
		if(PlayerInfo[playerid][pImaKucu])
		{
		    porez += 70+random(40);
		    struja += 30+random(40);
		    voda += 20+random(20);
		    plin += 35+random(40);
		    TV += 5+random(20);
		}
		if(PlayerInfo[playerid][pImaStan])
		{
		    porez += 50+random(40);
		    struja += 20+random(40);
		    voda += 15+random(20);
		    plin += 30+random(40);
		    TV += 5+random(20);
		}
		if(PlayerInfo[playerid][pImaFirmu])
		{
            porez += 200+random(100);
		    struja += 100+random(100);
		    plin += 50+random(50);
		}
		rezije = struja+voda+TV+plin+porez;
		if(PlayerInfo[playerid][pClan] > 0) dodatak += 600+random(1000);
		if(PlayerInfo[playerid][pPosao] > 0) placa += 500+random(300)+(PlayerInfo[playerid][pPayDayPoeni]*7)+(PlayerInfo[playerid][pLevel]*149);
		else placa += 200+random(120)+(PlayerInfo[playerid][pPayDayPoeni]*3)+(PlayerInfo[playerid][pLevel]*79);
		trosak += rezije + PlayerInfo[playerid][pIznosRate] + (PlayerInfo[playerid][pKazne]*2);
		prihod += dodatak + placa;
		zarada = prihod - trosak;
		PlayerInfo[playerid][pNovacBanka] += zarada;
		if(PlayerInfo[playerid][pIznosKredita] > 0)
		{
			if(PlayerInfo[playerid][pIznosKredita] >= PlayerInfo[playerid][pIznosRate])
			{
				PlayerInfo[playerid][pIznosKredita] -= PlayerInfo[playerid][pIznosRate];
			}
			else
			{
				PlayerInfo[playerid][pIznosKredita] = 0;
				PlayerInfo[playerid][pIznosRate] = 0;
			}
		}
		if(PlayerInfo[playerid][pNovacBanka] < 0)
		{
		    SCM(playerid,-1,""CRVENA"Zbog minusa na racunu u banci, kaznjen si kaznom zatvora u trajanju od 10 minuta");
            TogglePlayerControllable(playerid,1);
	        SetPlayerPos(playerid,264.5197,77.4091,1001.5605);
	        SetPlayerInterior(playerid,6);
	        Uhicen[playerid] = 0;
	        Vezan[playerid] = 0;
	        PlayerInfo[playerid][pZatvor] = 1;
	        PlayerInfo[playerid][pZatvorenVrijeme] += 600;
	        foreach(Player,i)
	        {
                if(Vuce[i] == playerid) {KillTimer(TimerPovlacenje[i]); TimerPovlacenje[i] = 0;}
			}
	        SetPlayerWantedLevel(playerid,0);
	        KillTimer(TimerWanted[playerid]);
	        TimerWanted[playerid] = 0;
	        TimerArrest[playerid] = SetTimerEx("Zatvor",5000,true,"d",playerid);
		}
		new string[1000],string1[512],string2[512];
		format(string1,sizeof(string1),""CPLAVA"----------------- Izvjesce banke -----------------\n\n "BIJELA"Bankovno izvjesce za proteklo razdoblje:\n\n Prihodi: (%d$)\n Iznos Place: %d$\n Dodatak: %d$",prihod,placa,dodatak);
		format(string2,sizeof(string2),""BIJELA" \n\n Rashodi: (%d$)\n Kazne: %d$\n Porez: %d$\n Struja: %d$\n Voda: %d$\n Plin: %d$\n TV: %d$\n Iznos rate kredita: %d$\n\n Stanje Racuna: %d$\n Stanje kredita: %d$\n\n"CPLAVA"------------------------------------------------------",trosak,PlayerInfo[playerid][pKazne]*2,porez,struja,voda,plin,TV,PlayerInfo[playerid][pIznosRate],PlayerInfo[playerid][pNovacBanka],PlayerInfo[playerid][pIznosKredita]);
		format(string,sizeof(string),"%s%s",string1,string2);
		ShowPlayerDialog(playerid,DIALOG_PAY,DIALOG_STYLE_MSGBOX,""BIJELA"Izvjesce",string,"OK","");
	}
	DobioPay[playerid] = 1;
	return 1;
}

