	}
	if(newstate == PLAYER_STATE_ONFOOT && (oldstate == PLAYER_STATE_DRIVER || oldstate == PLAYER_STATE_PASSENGER))
	{
	    if((postavioradio[playerid] == -2 || postavioradio[playerid] > -1) && Slusa[playerid] == 1)
	    {
	    StopAudioStreamForPlayer(playerid);
	    Slusa[playerid] = 0;
	    }
	    else if(postavioradio[playerid] == -1 && Slusa[playerid] == 1)
	    {
	    StopAudioStreamForPlayer(playerid);
