					PlayerInfo[i][pRespekti] = 0;
					PlayerInfo[i][pPotrebnoDoLvlUp] = PlayerInfo[i][pLevel]*2 + 4;
					SetPlayerScore(i, PlayerInfo[i][pLevel]);
