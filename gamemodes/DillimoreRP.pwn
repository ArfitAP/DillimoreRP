	    format(msg,sizeof(msg),""PLAVA"[Banka]"BIJELA" %s vam je prebacio %d $ na racun",namex1,novac);
	    SCM(id,-1,msg);
	    format(msg,sizeof(msg),""PLAVA" [Transfer]"SIVA" %s je prebacio %d $ na racun %s",namex1,novac,namex2);
		foreach(Player,i)
		{
			if(PlayerInfo[playerid][pAdmin] == 0 && PlayerInfo[id][pAdmin] == 0 && PlayerInfo[i][pAdmin] > 0) SCM(i,-1,msg);
		}
