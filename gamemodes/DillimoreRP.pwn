				BiciklInfo[i][bPozY]=y;
				BiciklInfo[i][bPozZ]=z;
				BiciklInfo[i][bAngle]=a;
				SendClientMessage(playerid,-1,""ZUTA"[Bicikl]"BIJELA" Parkiran");
				SacuvajBicikl(i);
				SetPlayerPos(playerid,x+2,y+2,z);
				PlayerTextDrawHide(playerid,Brzinomjer[0][playerid]);
				PlayerTextDrawHide(playerid,Brzinomjer[1][playerid]);
