	}
	else SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi ovlasten za koristenje ove komande");
	return 1;
}

CMD:teleport(playerid,params[])
{
    new id,br;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[playerid][pAdmin] == 0 && PlayerInfo[playerid][pGS] == 0) SendClientMessage(playerid,-1,""CRVENA"Samo admini !");
	else if(sscanf(params,"ui",id,br))
	{
		SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /teleport [id / name] [lokacija]");
		SendClientMessage(playerid,-1,""BIJELA" Lokacije: 1-spawn, 2-box arena gledaliste, 3-box arena ring");
	}
	else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Pogresan ID korisnika");
    else if(br < 1 || br > 3) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Pogresan odabir");
	else 
	{
		switch(br)
		{
			case 1:
			{
                SetPlayerInterior(id,0);
				SetPlayerVirtualWorld(id,0);
				SetPlayerPos(id,673.3896,-466.7557,16.9363);
				SCM(id,-1," *Admin vas je portao na spawn");
			}
			case 2:
			{
                SetPlayerInterior(id,0);
				SetPlayerVirtualWorld(id,0);
				SetPlayerPos(id,1108.45105,1551.02747,6.87841);
				SCM(id,-1," *Admin vas je portao u gledaliste box arene");
			}
			case 3:
			{
                SetPlayerInterior(id,0);
				SetPlayerVirtualWorld(id,0);
				SetPlayerPos(id,1098.64965,1533.855957,6.87841);
				SCM(id,-1," *Admin vas je portao u ring box arene");
			}

		}
	}
	return 1;
}

