public OnPlayerDeath(playerid, killerid, reason)
{
	if(killerid != INVALID_PLAYER_ID)
	{
		if(UWaru[playerid] && UWaru[killerid])
		{
            new msg[150],name[32],name2[32],br1=0,br2=0;
            GetPlayerName(killerid,name,sizeof(name));
            GetPlayerName(playerid,name2,sizeof(name2));
            
            for(new i; i<MAX_PLAYERS; i++)
			{
			    if(IsPlayerConnected(i))
			    {
	                if(UWaru[i] && PlayerInfo[i][pClan] == Ekipa1) br1++;
	                else if(UWaru[i] && PlayerInfo[i][pClan] == Ekipa2) br2++;
	            }
			}
            
            if(PlayerInfo[killerid][pClan] == Ekipa1)
            {
                format(msg,sizeof(msg),""PLAVA" *%s je ubio %s !", name, name2);
				if(br1 < br2)
				{
				    new Float:bod;
				    bod = br2/br1;
					E1K = E1K + bod;
				}
				else E1K = E1K + 1.0;
			}
			else if(PlayerInfo[killerid][pClan] == Ekipa2)
            {
                format(msg,sizeof(msg),""CRVENA" *%s je ubio %s !", name, name2);
				if(br1 > br2)
				{
				    new Float:bod;
				    bod = br1/br2;
					E2K = E2K + bod;
				}
				else E2K = E2K + 1.0;
			}
			
			format(name, sizeof(name), "~w~Bodova: %.3f",E1K);
	    	format(name2, sizeof(name2), "~w~Bodova: %.3f",E2K);
			
			for(new i; i<MAX_PLAYERS; i++)
			{
			    if(IsPlayerConnected(i))
			    {
                	if(UWaru[i] == 1)
					{
						SCM(i,-1,msg);
						PlayerTextDrawSetString(i,TextDrawWarE1B[i],name);
						PlayerTextDrawSetString(i,TextDrawWarE2B[i],name2);
					}
                }
            }
            return 1;
		}

		else if(PlayerInfo[killerid][pClan] != 1)
		{
		    new msg[150],namex[32];
			GetPlayerName(killerid,namex,sizeof(namex));
		    if(PlayerInfo[playerid][pClan] != 1)
			{
			    SCM(killerid,-1,""CRVENA"Pocinili ste zlocin: ubojstvo");
				format(msg,sizeof(msg),""PLAVA"[Centrala]"SIVA" %s je pocinio zlocin: ubojstvo",namex);
				SetPlayerWantedLevel(killerid,GetPlayerWantedLevel(killerid)+2);
				TimerWanted[killerid] = SetTimerEx("Wanted",120000,true,"d",killerid);
			}
			else
			{
			    SCM(killerid,-1,""CRVENA"Pocinili ste zlocin: ubojstvo predstavnika zakona");
                format(msg,sizeof(msg),""PLAVA"[Centrala]"SIVA" %s je pocinio zlocin: ubojstvo predstavnika zakona",namex);
                SetPlayerWantedLevel(killerid,GetPlayerWantedLevel(killerid)+6);
				TimerWanted[killerid] = SetTimerEx("Wanted",120000,true,"d",killerid);
			}
			foreach(Player,j)
			{
				if(PlayerInfo[j][pClan] == 1) SCM(j,-1,msg);
			}
		}
		PlayerInfo[killerid][pUbojstava]++;
		PlayerInfo[playerid][pSmrti]++;
	}
	PlayerTextDrawHide(playerid,Brzinomjer[0][playerid]);
	PlayerTextDrawHide(playerid,Brzinomjer[1][playerid]);
	Mlijeko[playerid] = 0;
	if(ecp[playerid] != 0)
	{
		Prisustvuje[playerid] = false;
		vorcp[playerid] = 0;

	}
	if(PlayerKanta[playerid] == 0)
	{
		Kanta1 = 0;
		DestroyObject(MlijekoKanta[0]);
		MlijekoKanta[0] = CreateObject(2902,1006.4000000,-319.6000100,74.4000000,0.0000000,90.0000000,0.0000000);
	}
	else if(PlayerKanta[playerid] == 1)
	{
		Kanta2 = 0;
		DestroyObject(MlijekoKanta[1]);
		MlijekoKanta[1] = CreateObject(2902,1006.4000000,-318.8999900,74.4000000,0.0000000,90.0000000,0.0000000);
	}
	else if(PlayerKanta[playerid] == 2)
	{
		Kanta3 = 0;
		DestroyObject(MlijekoKanta[2]);
		MlijekoKanta[2] = CreateObject(2902,1006.4000000,-320.2000100,74.4000000,0.0000000,90.0000000,0.0000000);
	}
	PlayerKanta[playerid] = -1;
	MlijekoProdaja[playerid] = 0;
	RemovePlayerAttachedObject(playerid, 0);
	UPljacki[playerid] = 0;
	IzvrsioPljacku[playerid] = 0;
	return 1;
}

