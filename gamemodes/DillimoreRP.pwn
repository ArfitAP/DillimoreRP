CMD:sms(playerid,params[])
{
    new broj,text[70];
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else if(PlayerInfo[playerid][pMobilni] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemas mobitel");
    else if(PlayerInfo[playerid][pMobKredit] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemas novca na racunu");
    else if(sscanf(params,"is[70]",broj,text))	SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /sms [broj mobitela] [text]");
    else
    {
    	new i;
		for(i=0; i<MAX_PLAYERS; i++)
    	{
     		if(IsPlayerConnected(i))
		    {
				if(PlayerInfo[i][pMobilni] == broj)
				{
				    if(i == playerid)  return SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne mozes sam sebi slati poruke");
				    new sat,minuta,sekunda,name1[32],msg1[100],name2[32],msg[100];
				    gettime(sat,minuta,sekunda);
				    GetPlayerName(playerid,name1,sizeof(name1));
				    GetPlayerName(i,name2,sizeof(name2));
				    format(msg,sizeof(msg),""TROZA" *%s uzima mobitel i pise poruku",name1);
					ProxDetector(20.0, playerid,msg,-1,-1,-1,-1,-1);
				    format(msg,sizeof(msg),""ZUTA"SMS poruka - Posiljatelj: %s (%d) - %d:%d",name1,PlayerInfo[playerid][pMobilni],sat,minuta);
				    SCM(i,-1,msg);
				    format(msg,sizeof(msg),""ZUTA"SMS poruka - Primatelj: %s (%d) - %d:%d",name2,PlayerInfo[i][pMobilni],sat,minuta);
				    SCM(playerid,-1,msg);
				    format(msg,sizeof(msg),""ZUTA" SMS poruka - Salje: %s - Prima: %s",name1,name2);
				    format(msg1,sizeof(msg1),""SIVA" *%s.",text);
				    PlayerInfo[playerid][pMobKredit]--;
					foreach(Player,j)
					{
						if(PlayerInfo[playerid][pAdmin] == 0 && PlayerInfo[i][pAdmin] == 0 && PlayerInfo[j][pAdmin] > 0)
						{
							SCM(j,-1,msg);
							SCM(j,-1,msg1);
						}
					}
				    SCM(playerid,-1,msg1);
					return SCM(i,-1,msg1);
				}
			}
		}
		SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan broj mobitela");
	}
	return 1;
}

