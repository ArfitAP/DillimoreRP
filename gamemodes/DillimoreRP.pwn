		    for(new i; i<MAX_PLAYERS; i++)
		    {
		      if(IsPlayerConnected(i))
		      {
		        	if(IsPlayerInVehicle(i, car))
		        	{
                        PlayAudioStreamForPlayer(i,"http://www.radio-banovina.hr/popup/playlist.php?name=listen.pls");
                        Slusa[i] = 1;
		        	}
		      }
		    }
		}
		else
		{
        	new namex[32],string[60],Float:x,Float:y,Float:z,Float:a;
		    GetPlayerName(playerid,namex,sizeof(namex));
		    format(string,sizeof(string),""TROZA" *%s postavlja radio",namex);
		    ProxDetector(20.0, playerid,string,-1,-1,-1,-1,-1);
		    GetPlayerPos(playerid,x,y,z);
		    GetPlayerFacingAngle(playerid,a);
	    	SCM(playerid,-1,""PLAVA"Postavio si radio, da ga maknes koristi: /radio");
     		postavioradio[playerid] = CreateObject(2103,x+0.5,y+0.4,z-1,0.0000000,0.0000000,a);
     		RadioKo[playerid][0] = x;
     		RadioKo[playerid][1] = y;
     		RadioKo[playerid][2] = z;
		}
	}
	else
	{
	    if(IsPlayerInAnyVehicle(playerid))
		{
			if(postavioradio[playerid] == -1)
		    {
                postavioradio[playerid] = -2;
                new car = GetPlayerVehicleID(playerid);
			    new namex[32],string[60];
			    GetPlayerName(playerid,namex,sizeof(namex));
			    format(string,sizeof(string),""TROZA" *%s je maknuo radio",namex);
			    ProxDetector(5.0, playerid,string,-1,-1,-1,-1,-1);
			    for(new i; i<MAX_PLAYERS; i++)
			    {
			      if(IsPlayerConnected(i))
			      {
			        	if(IsPlayerInVehicle(i, car))
			        	{
	                        StopAudioStreamForPlayer(i);
	                        Slusa[i] = 0;
			        	}
			      }
			    }
			    return 1;
			}
		}
        new Float:x,Float:y,Float:z;
        GetObjectPos(postavioradio[playerid],x,y,z);
	    if(IsPlayerInRangeOfPoint(playerid,5.0,x,y,z))
	    {
	        if(IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Izadi iz vozila");
	    	SCM(playerid,-1,""PLAVA"Maknuo si radio, da ga postavis koristi: /radio");
	    	DestroyObject(postavioradio[playerid]);
	    	
	    	foreach(Player,j)
      		{
				if(IsPlayerInRangeOfPoint(j,20.0,RadioKo[playerid][0],RadioKo[playerid][1],RadioKo[playerid][2]) && RadioKo[playerid][0] != 0.0)
				{
					StopAudioStreamForPlayer(j);
					Slusa[j] = 0;
			    }
			}

	    	postavioradio[playerid] = -2;
	    	new msg[50],namex[32];
	    	new car = GetPlayerVehicleID(playerid);
			GetPlayerName(playerid,namex,sizeof(namex));
			format(msg, sizeof(msg), ""TROZA" *%s je maknuo radio", namex);
			ProxDetector(20.0, playerid,msg,-1,-1,-1,-1,-1);
			RadioKo[playerid][0] = 0.0;
     		RadioKo[playerid][1] = 0.0;
     		RadioKo[playerid][2] = 0.0;
     		for(new i; i<MAX_PLAYERS; i++)
		    {
		      if(IsPlayerConnected(i))
		      {
		        	if(IsPlayerInVehicle(i, car))
		        	{
		        	    Slusa[i] = 0;
                        StopAudioStreamForPlayer(i);
					}
		      }
		    }
     	}
     	else SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi u blizini radia");
	}
    return 1;
}

