	    {
			DisablePlayerCheckpoint(playerid);
			SendClientMessage(playerid,-1,""PLAVA"[Dojenje]"BIJELA" Posao otkazan");
			Mlijeko[playerid] = 0;
		    if(PlayerKanta[playerid] == 0)
		    {
				Kanta1 = 0;
				DestroyObject(MlijekoKanta[0]);
				MlijekoKanta[0] = CreateObject(2902,1006.4000000,-319.6000100,74.4000000,0.0000000,90.0000000,0.0000000);
			}
			
			else if(PlayerKanta[playerid] == 1)
		    {
				Kanta2 = 0;
				DestroyObject(MlijekoKanta[1]);
				MlijekoKanta[1] = CreateObject(2902,1006.4000000,-318.8999900,74.4000000,0.0000000,90.0000000,0.0000000);
			}
			
			else if(PlayerKanta[playerid] == 2)
		    {
				Kanta3 = 0;
				DestroyObject(MlijekoKanta[2]);
				MlijekoKanta[2] = CreateObject(2902,1006.4000000,-320.2000100,74.4000000,0.0000000,90.0000000,0.0000000);
			}

			PlayerKanta[playerid] = -1;
			MlijekoProdaja[playerid] = 0;
		}
	}
	
	if(vehicleid == TSCVozila)
			{
				if(PlayerInfo[playerid][pClan] != 2)
				{
					GameTextForPlayer(playerid,"~l~TSC~n~~r~zakljucano",2000,3);
					SetPlayerPos(playerid,x,y,z);
                    TogglePlayerControllable(playerid,0);
				    TogglePlayerControllable(playerid,1);
					RemovePlayerFromVehicle(playerid);
				}
			}

	else if(vehicleid == ESCVozila)
			{
				if(PlayerInfo[playerid][pClan] != 3)
				{
					GameTextForPlayer(playerid,"~b~ESC~n~~r~zakljucano",2000,3);
					SetPlayerPos(playerid,x,y,z);
                    TogglePlayerControllable(playerid,0);
				    TogglePlayerControllable(playerid,1);
					RemovePlayerFromVehicle(playerid);
				}
			}

	else if(vehicleid == MBVozila)
			{
				if(PlayerInfo[playerid][pClan] != 4)
				{
					GameTextForPlayer(playerid,"~r~MB~n~zakljucano",2000,3);
					SetPlayerPos(playerid,x,y,z);
                    TogglePlayerControllable(playerid,0);
				    TogglePlayerControllable(playerid,1);
					RemovePlayerFromVehicle(playerid);
				}
			}
	else
	{
		for(i=0;i<MAX_VOZILA;i++)
	 	{
	 	    if(!ispassenger)
	 	    {
				if(vehicleid == VoziloInfo[i][vID])
				{
				    if(PlayerInfo[playerid][pVozilo] != i && VoziloInfo[i][vZakljucan] == 1 && PlayerInfo[playerid][pAdmin] == 0)
				    {
						new msg[128];
						format(msg,sizeof(msg),""CRVENA"Vozilo zakljucano:"BIJELA" Vlasnik: %s",VoziloInfo[i][vVlasnik]);
					    SendClientMessage(playerid,-1,msg);
						break;
					}
				}
				else if(vehicleid == MotorInfo[i][mID])
				{
					if(PlayerInfo[playerid][pMotor] != i && MotorInfo[i][mZakljucan] == 1 && PlayerInfo[playerid][pAdmin] == 0)
					{
						new msg[128];
						format(msg,sizeof(msg),""CRVENA"Motor zakljucan:"BIJELA" Vlasnik: %s",MotorInfo[i][mVlasnik]);
					    SendClientMessage(playerid,-1,msg);
						break;
					}
				}
				else if(vehicleid == BiciklInfo[i][bID])
				{
					if(PlayerInfo[playerid][pBicikl] != i && BiciklInfo[i][bZakljucan] == 1 && PlayerInfo[playerid][pAdmin] == 0)
					{
						new msg[128];
						format(msg,sizeof(msg),""CRVENA"Bicikl zakljucan lancem:"BIJELA" Vlasnik: %s",BiciklInfo[i][bVlasnik]);
					    SendClientMessage(playerid,-1,msg);
						break;
					}
				}
			}
			if(i == (MAX_VOZILA-1)) break;
		}
	}
	return 1;
}

