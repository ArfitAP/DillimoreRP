   	else if(UWaru[playerid] == 0) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi u waru.");
   	else if(Votao[playerid] == 1) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Vec si glasao.");
	else if(Votekick[0] > -1 && PlayerInfo[playerid][pClan] == Ekipa1)
	{
	    new msg[100],name[32];
		VKBod[0] -= 1;
		Votao[playerid] = 1;
		GetPlayerName(Votekick[0],name,sizeof(name));
		format(msg,sizeof(msg),""CRVENA"[WAR] za izbacivanje %s potrebno je jos glasova: %d. /votekick za glas",name,VKBod[0]);
		for(new i; i<MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i) && VKBod[0] > 0) if(Votekick[0] != i && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa1) SendClientMessage(i,-1,msg);
		}
		if(VKBod[0] <= 0)
		{
            format(msg,sizeof(msg),""CRVENA"[WAR] %s izbacen iz ekipe",name);
            for(new i; i<MAX_PLAYERS; i++)
			{
				if(IsPlayerConnected(i)) if(Votekick[0] != i && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa1)
				{
					SendClientMessage(i,-1,msg);
					Votao[i] = 0;
				}
			}
			Izbaciizwara(Votekick[0]);
			Votekick[0] = -1;
			VKBod[0] = -1;
		}
		return 1;
	}
	else if(Votekick[1] > -1 && PlayerInfo[playerid][pClan] == Ekipa2)
	{
        new msg[100],name[32];
		VKBod[1] -= 1;
		Votao[playerid] = 1;
		GetPlayerName(Votekick[1],name,sizeof(name));
		format(msg,sizeof(msg),""CRVENA"[WAR] za izbacivanje %s potrebno je jos glasova: %d. /votekick za glas",name,VKBod[1]);
		for(new i; i<MAX_PLAYERS; i++)
		{
			if(IsPlayerConnected(i) && VKBod[1] > 0) if(Votekick[1] != i && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa2) SendClientMessage(i,-1,msg);
		}
		if(VKBod[1] <= 0)
		{
            format(msg,sizeof(msg),""CRVENA"[WAR] %s izbacen iz ekipe",name);
            for(new i; i<MAX_PLAYERS; i++)
			{
				if(IsPlayerConnected(i)) if(Votekick[1] != i && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa2)
				{
					SendClientMessage(i,-1,msg);
					Votao[i] = 0;
				}
			}
			Izbaciizwara(Votekick[1]);
			Votekick[1] = -1;
			VKBod[1] = -1;
		}
		return 1;
	}
	else if(sscanf(params,"u",id)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /votekick [id / name]");
	{
 		if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Pogresan ID korisnika");
		else if(UWaru[id] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Igrac nije u waru.");
		else if(id == playerid) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne mozes glasati za sebe.");
        else if(PlayerInfo[id][pClan] != PlayerInfo[playerid][pClan]) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Igrac nije u tvojoj ekipi.");
        else
        {
            if(PlayerInfo[playerid][pClan] == Ekipa1)
			{
				new br=0;
				for(new i; i<MAX_PLAYERS; i++)
				{
				    if(IsPlayerConnected(i)) if(id != i && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa1) br++;
				}
				if(br > 1)
				{
					new msg[100],name[32];
					VKBod[0] = br/2;
					Votao[playerid] = 1;
					Votekick[0] = id;
					GetPlayerName(id,name,sizeof(name));
					format(msg,sizeof(msg),""CRVENA"[WAR] Pokrenuto je izbacivanje igraca %s, potrebno glasova: %d. /votekick za glas",name,VKBod[0]);
					for(new i; i<MAX_PLAYERS; i++)
					{
					    if(IsPlayerConnected(i)) if(id != i && i != playerid && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa1) SendClientMessage(i,-1,msg);
					}
					format(msg,sizeof(msg),""CRVENA"[WAR] Pokrenuo si izbacivanje igraca %s, vasi suigraci ce odluciti",name);
					SendClientMessage(playerid,-1,msg);
					SetTimer("Vote1",60000,false);
				}
				else SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nema dovoljno igraca u ekipi za glasanje");
			}
			else if(PlayerInfo[playerid][pClan] == Ekipa2)
			{
                new br=0;
				for(new i; i<MAX_PLAYERS; i++)
				{
				    if(IsPlayerConnected(i)) if(id != i && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa2) br++;
				}
				if(br > 1)
				{
					new msg[100],name[32];
					VKBod[1] = br/2;
					Votao[playerid] = 1;
					Votekick[1] = id;
					GetPlayerName(id,name,sizeof(name));
					format(msg,sizeof(msg),""CRVENA"[WAR] Pokrenuto je izbacivanje igraca %s, potrebno glasova: %d. /votekick za glas",name,VKBod[1]);
					for(new i; i<MAX_PLAYERS; i++)
					{
					    if(IsPlayerConnected(i)) if(id != i && i != playerid && UWaru[i] == 1 && PlayerInfo[i][pClan] == Ekipa2) SendClientMessage(i,-1,msg);
					}
					format(msg,sizeof(msg),""CRVENA"[WAR] Pokrenuo si izbacivanje igraca %s, vasi suigraci ce odluciti",name);
					SendClientMessage(playerid,-1,msg);
					SetTimer("Vote2",60000,false);
				}
				else SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nema dovoljno igraca u ekipi za glasanje");
			}
		}
	}
	
    return 1;
}

