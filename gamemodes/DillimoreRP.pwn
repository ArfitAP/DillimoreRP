forward PljackaIzvrsenje(playerid);
public PljackaIzvrsenje(playerid)
{
	new msg[128],namex[32];
	TogglePlayerControllable(playerid,1);
	ClearAnimations(playerid);
	if(IsPlayerInRangeOfPoint(playerid, 20.0, 2315.0730,-16.4404,26.7496)) UPljacki[playerid] = 1 + random(5000);
	else
	{
		UPljacki[playerid] = FirmaInfo[UsaoUFirmu[playerid]][fZarada];
		format(msg,sizeof(msg),""CRVENA"[Pljacka]"SIVA" Netko je opljackao sef vase firme i odnio %d $, policija je obavjesetna!",UPljacki[playerid]);
		if(GetPlayerID(FirmaInfo[UsaoUFirmu[playerid]][fVlasnik]) > -1) SCM(GetPlayerID(FirmaInfo[UsaoUFirmu[playerid]][fVlasnik]),-1,msg);
	}
	FirmaInfo[UsaoUFirmu[playerid]][fZarada] = 0;
	
	GetPlayerName(playerid,namex,sizeof(namex));
	format(msg,sizeof(msg),""CRVENA" [Pljacka] "BIJELA"Izvrsio si pljacku i pronasao %d $",UPljacki[playerid]);
	SCM(playerid,-1,msg);
	SCM(playerid,-1,""CRVENA" [Pljacka] "BIJELA"Nakon sto te policija prestane loviti odnesi torbu u ormar sa opremom!");
	format(msg,sizeof(msg),""PLAVA"[Centrala]"SIVA" %s je prijavljen za pljacku sefa !",namex);
    foreach(Player,i)
 	{
		if(PlayerInfo[i][pClan] == 1) SCM(i,-1,msg);
	}
	SetPlayerWantedLevel(playerid,6);
	IzvrsioPljacku[playerid] = 1;
	SetPlayerAttachedObject(playerid, 0, 1550, 1,0.0,-0.34,0.0,0.0,90.0,0.0);
	Pljacaka = 0;
	TimerPlj = SetTimer("Plj",1200000,false);
	TimerWanted[playerid] = SetTimerEx("Wanted",120000,true,"d",playerid);
	return 1;
}

