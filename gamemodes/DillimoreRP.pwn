CMD:selldrugs(playerid,params[])
{
	new id,cijena,kolicina,Float:x,Float:y,Float:z;
	GetPlayerPos(playerid,x,y,z);
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
	else if(PlayerInfo[ playerid ][ pPosao ] != 5) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi diler droge!");
	else if(sscanf(params,"uii",id,kolicina,cijena)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Koristi: /selldrugs [id / name] [kolicina] [cijena]");
	else if(IsPlayerInAnyVehicle(playerid)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Ne smijes biti u vozilu");
	else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Pogresan ID korisnika");
	else if(cijena<1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Pogresna cijena droge");
	else if(kolicina<1 || kolicina > PlayerInfo[playerid][pDroga]) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Pogresna kolicina droge");
	else if(!IsPlayerInRangeOfPoint(id,7.0,x,y,z)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Igrac nije u vasoj blizini");
	else if(id == playerid) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Ne mozes sebi ponuditi drogu");
	else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else
	{
		new namex[32],msg[75];
		GetPlayerName(id,namex,sizeof(namex));
		format(msg,sizeof(msg),""NARANCASTA" Ponudio si %s %d grama droge za %d$",namex,kolicina,cijena);
		SendClientMessage(playerid,-1,msg);
		GetPlayerName(playerid,namex,sizeof(namex));
		format(msg,sizeof(msg),""NARANCASTA" %s vam je ponudio %d grama droge za %d$, /prihvati da prihvatis",namex,kolicina,cijena);
		SendClientMessage(id,-1,msg);
		PonudioDrogu[id] = playerid;
		PonudenaCijenaDroge[id] = cijena;
		PonudenaKolicinaDroge[id] = kolicina;
	}
	return 1;
}

CMD:usedrugs(playerid,params[])
{
	new msg[50],namex[32],Float:hp;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
