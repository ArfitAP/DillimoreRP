	else if(litara <= 0.0) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Upisi ispravan broj litara");
	else if(GetPlayerMoney(playerid) < (litara * 3)) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemas dovoljno novca");
	else if(VoziloJeBicikl(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne mozes tociti gorivo u bicikl");
	else if(engine == 1) return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras ugasiti motor vozila da bi tocio gorivo u rezervoar");
	else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else
	{
	    new i = GetPlayerVehicleID(playerid);
		if(VoziloJeMotor(i) && ((Gorivo[i] + litara) > 15))  return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Toliko goriva ne stane u rezervoar");
        else if(VoziloJeKamion(i) && ((Gorivo[i] + litara) > 60))  return SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Toliko goriva ne stane u rezervoar");
