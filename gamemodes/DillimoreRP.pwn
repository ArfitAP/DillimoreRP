CMD:call(playerid,params[])
{
	new odabir;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[playerid][pMobilni] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemas mobitel");
    else if(PlayerInfo[playerid][pMobKredit] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemas novca na racunu");
    else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else if(sscanf(params,"d",odabir))
	{
		SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /call [odabir ili broj mobitela]");
		SendClientMessage(playerid,-1,"Dostupan odabir: 1-mehanicar, 2-hitna");
	}
	else if(odabir == 1)
	{
	    new namex[32], msg[55];
		PozvaoMehanicara = playerid;
		GetPlayerName(playerid,namex,sizeof(namex));
		format(msg,sizeof(msg),""PPLAVA"%s je pozvao mehanicara, /prihvati da prihvatis poziv", namex);
		foreach(Player,i)
		{
			if(PlayerInfo[i][pPosao] == 4) SendClientMessage(i,-1,msg);
		}
		SendClientMessage(playerid,-1,""PPLAVA"Pozvali ste mehanicara pricekajte odgovor...");
		PlayerInfo[playerid][pMobKredit]--;
	}
	else if(odabir == 2)
	{
	    new namex[32], msg[55];
		PozvaoHitnu = playerid;
		GetPlayerName(playerid,namex,sizeof(namex));
		format(msg,sizeof(msg),""PPLAVA"%s je pozvao Hitnu pomoc, /prihvati da prihvatis poziv", namex);
		foreach(Player,i)
		{
			if(PlayerInfo[i][pClan] == 5) SendClientMessage(i,-1,msg);
		}
		SendClientMessage(playerid,-1,""PPLAVA"Pozvali ste hitnu pomoc pricekajte odgovor...");
		PlayerInfo[playerid][pMobKredit]--;
	}
	else
	{
	    if(RazgovaraNaMobitel[playerid] > -1) return SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Vec razgovaras na mobitel");
	    new i;
		for(i=0; i<MAX_PLAYERS; i++)
    	{
     		if(IsPlayerConnected(i))
		    {
				if(PlayerInfo[i][pMobilni] == odabir)
				{
				    if(i == playerid)  return SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne mozes sam sebe zvati na mobitel");
				    if(Zove[i] > -1 || RazgovaraNaMobitel[i] > -1) return SCM(playerid,-1,""SIVA" Zauzeto !!!");
				    new namex[32],msg[75],string[45];
				    
				    GetPlayerName(i,namex,sizeof(namex));
				    format(msg,sizeof(msg),""ZUTA"Pozvao si %s, cekaj odgovor",namex);
				    SCM(playerid,-1,msg);
				    
				    GetPlayerName(playerid,namex,sizeof(namex));
				    format(string,sizeof(string),""TROZA" *%s uzima mobitel",namex);
				    ProxDetector(20.0, playerid,string,-1,-1,-1,-1,-1);
				    
				    
				    format(msg,sizeof(msg),""ZUTA"Dolazni poziv od %s, /pickup",namex);
				    
				    PlayerInfo[playerid][pMobKredit]--;
					Zove[i] = playerid;
					if(!IsPlayerInAnyVehicle(playerid)) SetPlayerSpecialAction(playerid,SPECIAL_ACTION_USECELLPHONE);
					return SCM(i,-1,msg);
				}
			}
		}
		SCM(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan broj mobitela");
	}
	return 1;
}

