public OnPlayerExitVehicle(playerid, vehicleid)
{
    PlayerTextDrawHide(playerid,Brzinomjer[0][playerid]);
    PlayerTextDrawHide(playerid,Brzinomjer[1][playerid]);
    KillTimer(TimerGorivo[playerid]);
    TimerGorivo[playerid] = 0;
    Pregledava[playerid] = 0;
    if(DostavaPizze[playerid]==1)
    {
		SendClientMessage(playerid,-1,""PLAVA"[Dostava]"BIJELA"Ne Smijes silaziti s motora, dostava prekinuta");
		DostavaPizze[playerid]=0;
		DisablePlayerCheckpoint(playerid);
	}
	else if(Polaganje[playerid] == 1)
	{
		Polaganje[playerid] = 0;
		PolaganjeStanica[playerid] = 0;
		DisablePlayerCheckpoint(playerid);
		new engine, lights, alarm, doors, bonnet, boot, objective;
		GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
		SetVehicleParamsEx(vehicleid, 0, lights, alarm, doors, bonnet, boot, objective);
		Gorivo[vehicleid] = 40.0;
		SetVehicleToRespawn(vehicleid);
		SCM(playerid,-1,""CRVENA"Zbog napustanja vozila, pao si na testu");
		KillTimer(TimerSkola[playerid]);
		KillTimer(TimerPolaganje[playerid]);
		TimerSkola[playerid] = 0;
		TimerPolaganje[playerid] = 0;
	}
	else if(Polje[playerid]>0)
	{
            SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ne smijes napustati stroj, posao zavrsen!");
            Polje[playerid] = 0;
            PoljeStanica[playerid] = 0;
			DisablePlayerCheckpoint(playerid);
	}
	return 1;
}

