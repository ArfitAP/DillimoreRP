CMD:war(playerid,params[])
{
	new side1, side2, time;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[playerid][pAdmin] == 0) SendClientMessage(playerid,-1,""CRVENA"Samo admini");
   	else if(War == 1) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" War je vec pokrenut.");
    else if(sscanf(params,"iii",side1,side2,time))
	{
		SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /war [tim 1] [tim 2] [trajanje u minutama]");
		SendClientMessage(playerid,-1,""BIJELA" Timovi: 1-SAPD, 2-TSC, 3-ESC, 4-MB, 5-MD");
	}
	else if(side1 < 1 || side1 > 5 || side2 < 1 || side2 > 5 || side1 == side2) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Pogresan odabir timova!");
	else if(time < 3 || time > 20) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Minimalno trajanje wara je 3 minute, a maksimalno 20 minuta.");
	else
	{
		new br1=0,br2=0;
		foreach(Player,i)
		{
			if(PlayerInfo[i][pClan] == side1) br1++;
			if(PlayerInfo[i][pClan] == side2) br2++;
		}
		
		if(br1 == 0 || br2 == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nema clanova odabrane organizacije online.");
		else
		{
            War = 1;
            Wartrajanje = time * 60;
            Ekipa1 = side1;
            Ekipa2 = side2;
            E1K = 0;
            E2K = 0;
            SendClientMessage(playerid,-1,""CRVENA"[WAR]"BIJELA" Pokrenuo si war, sudionici su obavjesteni.");
            foreach(Player,i)
			{
				if(PlayerInfo[i][pClan] == side1 || PlayerInfo[i][pClan] == side2)
				{
					SendClientMessage(i,-1,""CRVENA"[WAR]"BIJELA" Pozvani ste u war koji pocinje za 2 minute, koristite /warprihvati da sudjelujete");
					Warpozvan[i] = 1;
				}
			}
			SetTimer("PocniWar",120000,false);
		}
	}
	return 1;
}

