forward EventNapusti(i);
public EventNapusti(i)
{
				UWaru[i] = 0;
    			Warpozvan[i] = 0;
    			PlayerInfo[i][pPridruzeneventu] = 0;
				DisablePlayerCheckpoint(i);
				Prisustvuje[i] = false;
				ecp[i] = 0;
				eecp[i] = 0;
				vorcp[i] = 0;
				PlayerInfo[i][pCistacX] = 0;
				PlayerInfo[i][pCistacY] = 0;
				PlayerInfo[i][pCistacZ] = 0;
				
				PlayerTextDrawHide(i,TextDrawCP[i]);
                PlayerTextDrawHide(i,TextDrawWarE1[i]);
                PlayerTextDrawHide(i,TextDrawWarE2[i]);
                PlayerTextDrawHide(i,TextDrawWarE1B[i]);
                PlayerTextDrawHide(i,TextDrawWarE2B[i]);
                PlayerTextDrawHide(i,TextDrawWarTime[i]);

                SetPlayerColor(i,COLOR_WHITE);
                SetPlayerArmour(i, 0);
                SetPlayerHealth(i, 100);
                ResetPlayerWeapons(i);

                if(PlayerInfo[i][pSpawn] == 0)
				{
				    SetSpawnInfo(i,0,PlayerInfo[i][pSkin],673.3896,-466.7557,16.9363,92.1342,0,0,0,0,0,0);
					SetPlayerPos(i,673.3896,-466.7557,16.9363);
					SetPlayerFacingAngle(i,92.1342);
					SetPlayerInterior(i,0);
					SetPlayerVirtualWorld(i,0);
				}
				else if(PlayerInfo[i][pSpawn] == 1)
				{
				    SetSpawnInfo(i,0,PlayerInfo[i][pSkin],StanInfo[PlayerInfo[i][pStan]][sIzlazX],StanInfo[PlayerInfo[i][pStan]][sIzlazY],StanInfo[PlayerInfo[i][pStan]][sIzlazZ]+0.6,0,0,0,0,0,0,0);
				    SetPlayerInterior(i,StanInfo[PlayerInfo[i][pStan]][sInterijerID]);
					SetPlayerPos(i,StanInfo[PlayerInfo[i][pStan]][sIzlazX],StanInfo[PlayerInfo[i][pStan]][sIzlazY],StanInfo[PlayerInfo[i][pStan]][sIzlazZ]+1.0);
					SetPlayerVirtualWorld(i,StanInfo[PlayerInfo[i][pStan]][sVW]);
					UsaoUStan[i] = PlayerInfo[i][pStan];
				}
				else if(PlayerInfo[i][pSpawn] == 2)
				{
				    SetSpawnInfo(i,0,PlayerInfo[i][pSkin],KucaInfo[PlayerInfo[i][pKuca]][kIzlazX],KucaInfo[PlayerInfo[i][pKuca]][kIzlazY],KucaInfo[PlayerInfo[i][pKuca]][kIzlazZ]+0.6,0,0,0,0,0,0,0);
				    SetPlayerInterior(i,KucaInfo[PlayerInfo[i][pKuca]][kInterijerID]);
					SetPlayerPos(i,KucaInfo[PlayerInfo[i][pKuca]][kIzlazX],KucaInfo[PlayerInfo[i][pKuca]][kIzlazY],KucaInfo[PlayerInfo[i][pKuca]][kIzlazZ]+1.0);
					SetPlayerVirtualWorld(i,KucaInfo[PlayerInfo[i][pKuca]][kVW]);
					UsaoUKucu[i] = PlayerInfo[i][pKuca];
				}
				else if(PlayerInfo[i][pSpawn] == 3)
				{
				    switch(PlayerInfo[i][pClan])
				    {
						case 1:
						{
						    SetSpawnInfo(i,0,PlayerInfo[i][pSkin],610.8047,-587.4880,17.2266,271.0670,0,0,0,0,0,0);
							SetPlayerPos(i,610.8047,-587.4880,17.2266);
							SetPlayerFacingAngle(i,271.0670);
						}
						case 2:
						{
						    SetSpawnInfo(i,0,PlayerInfo[i][pSkin],721.2005,-441.5359,16.3359,263.9830,0,0,0,0,0,0);
							SetPlayerPos(i,721.2005,-441.5359,16.3359);
							SetPlayerFacingAngle(i,263.9830);
						}
						case 3:
						{
						    SetSpawnInfo(i,0,PlayerInfo[i][pSkin],875.2190,-608.5042,18.4219,87.1689,0,0,0,0,0,0);
							SetPlayerPos(i,875.2190,-608.5042,18.4219);
							SetPlayerFacingAngle(i,87.1689);
						}
						case 4:
						{
						    SetSpawnInfo(i,0,PlayerInfo[i][pSkin],786.5819,-643.3492,16.3359,357.7309,0,0,0,0,0,0);
							SetPlayerPos(i,786.5819,-643.3492,16.3359);
							SetPlayerFacingAngle(i,357.7309);
