    else if(lvl == 3 && PlayerInfo[id][pAdmin] == 3) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac vec ima odabrani admin level");
    else
    {
        new namex[32],msg[100];
        if(lvl>0)
		{
			GetPlayerName(playerid,namex,sizeof(namex));
			format(msg,sizeof(msg),""ZELENA"RCON Admin %s vam je postavio administrator lvl %d",namex,lvl);
	        SCM(id,-1,msg);
	        GetPlayerName(id,namex,sizeof(namex));
	        format(msg,sizeof(msg),""ZELENA"Postavio si %s administrator lvl %d",namex,lvl);
	        SCM(playerid,-1,msg);
			format(msg,sizeof(msg),""PLAVA"[Admin]"SIVA" %s je promoviran u administratora lvl %d od strane RCON-a",namex,lvl);
		    foreach(Player,i)
		    {
				if(i != playerid && i != id && (PlayerInfo[i][pAdmin]>0 || PlayerInfo[i][pGS])) SCM(i,-1,msg);
			}
		}
		else
		{
            GetPlayerName(playerid,namex,sizeof(namex));
			format(msg,sizeof(msg),""CRVENA"RCON Admin %s vas je skinuo s mjesta administratora",namex);
        	SCM(id,-1,msg);
            GetPlayerName(id,namex,sizeof(namex));
            format(msg,sizeof(msg),""CRVENA"Skinuo si %s s mjesta administratora",namex);
            SCM(playerid,-1,msg);
		}
		PlayerInfo[id][pAdmin] = lvl;
		PlayerInfo[id][pGS] = 0;
		if(PlayerInfo[id][pAdmin] > 0)
		{
			label[id] = Create3DTextLabel(""CPLAVA"[ ADMINISTRATOR ]",-1,0,0,0,30.0,0,0);
			Attach3DTextLabelToPlayer(label[id], id, 0.0, 0.0, 0.5);
		}
		else
		{
			Delete3DTextLabel(label[id]);
		}
	}
	return 1;
}

