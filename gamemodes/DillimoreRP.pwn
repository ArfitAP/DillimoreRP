		RazgovaraNaMobitel[playerid] = -1;
		TimerPoziv[playerid] = 0;
		TimerPoziv[RazgovaraNaMobitel[playerid]] = 0;
	}

	foreach(Player,i)
	{
		if(PonudioPizzu[i] == playerid) PonudioPizzu[i] = -1;

        if(PonudioRepair[i] == playerid) PonudioRepair[i] = -1;

        if(PonudioRefill[i] == playerid) PonudioRefill[i] = -1;

        if(PonudioDrogu[i] == playerid) PonudioDrogu[i] = -1;
	}
	
	SmetlarRuta[playerid]=0; SmetlarStanica[playerid]=0;
	KolicinaPizze[playerid]=0; PonudenaKolicinaPizze[playerid]=0; PonudioPizzu[playerid]=-1; DostavaPizze[playerid]=0;
	Polje[playerid] = 0; PoljeStanica[playerid] = 0; Mlijeko[playerid] = 0; PlayerKanta[playerid] = -1; MlijekoProdaja[playerid] = 0; ImaMlijeka[playerid] = 1;
	PonudioRepair[playerid] = -1; PonudioRefill[playerid] = -1; PonudenaCijenaRepaira[playerid] = 0; PonudenaCijenaRefilla[playerid] = 0; PrihvatioMechanic[playerid] = 0; PrihvatioMedic[playerid] = 0;
	PosadenihPolja[playerid] = 0; Sjeme[playerid] = 0; Urod[playerid] = 0; AdminVozilo[playerid] = -1; Taz[playerid] = 0;
	PonudioDrogu[playerid] = -1; PonudenaCijenaDroge[playerid] = 0; PonudenaKolicinaDroge[playerid]=0;
	LociraoVozilo[playerid] = 0; Vuce[playerid] = -1; UpalioGPS[playerid] = 0; PoslaoPitanje[playerid]=0;
	UsaoUFirmu[playerid] = -1; TocioLitara[playerid] = 0; LociraoPosao[playerid] = 0; DaoPrijedlog[playerid] = 0; PrijavioBug[playerid] = 0;
