    else if(sscanf(params,"u",id)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Koristi: /eject [id / name]");
    else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Pogresan ID korisnika");
    else if(id == playerid) SendClientMessage(playerid,-1,""CRVENA"[Greska] "BIJELA"Ne mozes sam sebe izbaciti iz vozila");
    else if(GetPlayerVehicleID(playerid) != GetPlayerVehicleID(id)) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Taj igrac nije u vasem vozilu");
    else if(GetPlayerVehicleSeat(playerid) != 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi vozac");
    else
    {
        RemovePlayerFromVehicle(id);
        OnPlayerExitVehicle(id, GetPlayerVehicleID(id));
    }
	return 1;
}

