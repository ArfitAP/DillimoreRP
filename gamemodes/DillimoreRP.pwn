CMD:posao(playerid,params[])
{
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[ playerid ][ pPosao ] != 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Vec imas posao");
	else if(IsPlayerInRangeOfPoint(playerid,5.0,713.2213,-439.6051,16.3359))
		{
		    if(PlayerInfo[ playerid ][ pLevel ] < 3) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti minimalno lvl 3 za ovaj posao");
		    else
		    {
			PlayerInfo[ playerid ][ pPosao ] = 1;
			PlayerInfo[ playerid ][ pPosaoUgovor ] = 0;
			SendClientMessage(playerid,-1,""PLAVA"Postali ste smetlar, koristite kamion i komandu /smece ");
			}
		}
		
	else if(IsPlayerInRangeOfPoint(playerid,5.0,700.4595,-662.6179,16.4372))
		{
		    if(PlayerInfo[ playerid ][ pLevel ] < 3) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti minimalno lvl 3 za ovaj posao");
		    else
		    {
			PlayerInfo[ playerid ][ pPosao ] = 2;
			PlayerInfo[ playerid ][ pPosaoUgovor ] = 0;
			SendClientMessage(playerid,-1,""PLAVA"Postali ste prodavac pizze, koristite motor i komandu /prodajpizzu ");
			}
		}
		
	else if(IsPlayerInRangeOfPoint(playerid,5.0,1102.8008,-299.2251,73.9922))
		{
			PlayerInfo[ playerid ][ pPosao ] = 3;
			PlayerInfo[ playerid ][ pPosaoUgovor ] = 0;
			SendClientMessage(playerid,-1,""PLAVA"Postali ste seljak, koristite traktor, kombajn i komande /ori, /sadi, /beri, /mlijeko ");
  		}
		
	else if(IsPlayerInRangeOfPoint(playerid,5.0,713.8514,-476.1755,16.3281))
		{
		    if(PlayerInfo[ playerid ][ pLevel ] < 4) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti minimalno lvl 4 za ovaj posao");
		    else
		    {
			PlayerInfo[ playerid ][ pPosao ] = 4;
			PlayerInfo[ playerid ][ pPosaoUgovor ] = 0;
			SendClientMessage(playerid,-1,""PLAVA"Postali ste mehanicar, koristite komande /repair, /refill, /otkaci, /prihvati ");
			}
		}

    else if(IsPlayerInRangeOfPoint(playerid,5.0,750.7517,-297.1574,10.8162))
		{
		    if(PlayerInfo[ playerid ][ pLevel ] < 2) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti minimalno lvl 2 za ovaj posao");
		    else
		    {
			PlayerInfo[ playerid ][ pPosao ] = 5;
			PlayerInfo[ playerid ][ pPosaoUgovor ] = 0;
			SendClientMessage(playerid,-1,""PLAVA"Postali ste diler drogom, koristite komande /sjeme, /sadi, /oberi, /selldrugs ");
			}
		}

	else SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti blizu mjesta zaposlenja");
	
	return 1;
}

CMD:otkaz(playerid,params[])
{
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(PlayerInfo[ playerid ][ pPosao ] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Ti niti nemas posao");
    else if(PlayerInfo[ playerid ][ pPosaoUgovor ] < 3)
    {
        new msg[150];
        format(msg,sizeof(msg),""CRVENA"Da bi dao otkaz moras odraditi ugovor u trajanju od 3 sata, odradio si %d sati",PlayerInfo[ playerid ][ pPosaoUgovor ]);
        SCM(playerid,-1,msg);
    }
	else
	{
	    if(PlayerInfo[ playerid ][ pPosao ] == 1)
	    {
	        new i;
	        for(i=0;i<2;i++)
	        {
				if(GetPlayerVehicleID(playerid) == SmetlarKamion[i]) {TogglePlayerControllable(playerid,1); RemovePlayerFromVehicle(playerid); OnPlayerExitVehicle(playerid, GetPlayerVehicleID(playerid));}
			}
		    DisablePlayerCheckpoint(playerid);
		    SmetlarRuta[playerid] = 0;
		    SmetlarStanica[playerid] = 0;
	    }

		if(PlayerInfo[ playerid ][ pPosao ] == 2)
	    {
	        new i;
	        for(i=0;i<2;i++)
	        {
				if(GetPlayerVehicleID(playerid) == PizzaMotor[i]) {TogglePlayerControllable(playerid,1); RemovePlayerFromVehicle(playerid); OnPlayerExitVehicle(playerid, GetPlayerVehicleID(playerid));}
			}
	    }

		if(PlayerInfo[ playerid ][ pPosao ] == 3)
	    {
	        new i;
	        for(i=0;i<7;i++)
	        {
				if(GetPlayerVehicleID(playerid) == SeljakVozila[i]) {TogglePlayerControllable(playerid,1); RemovePlayerFromVehicle(playerid); OnPlayerExitVehicle(playerid, GetPlayerVehicleID(playerid));}
			}
	    }

		if(PlayerInfo[ playerid ][ pPosao ] == 4)
	    {
	        new i;
	        for(i=0;i<2;i++)
	        {
				if(GetPlayerVehicleID(playerid) == MehanicarVozilo[i]) {TogglePlayerControllable(playerid,1); RemovePlayerFromVehicle(playerid); OnPlayerExitVehicle(playerid, GetPlayerVehicleID(playerid));}
			}
	    }
	    
	    PlayerInfo[ playerid ][ pPosao ] = 0;
	    PlayerInfo[ playerid ][ pPosaoUgovor ] = 0;
		SendClientMessage(playerid,-1,""CRVENA"[Otkaz]"BIJELA" Dali ste otkaz i sada ste nezaposleni");
	}
	return 1;
}

CMD:smece(playerid,params[])
{
	if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
	else if(PlayerInfo[ playerid ][ pPosao ] != 1) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi smetlar!");
	else if(GetPlayerVehicleID(playerid) != SmetlarKamion[0] && GetPlayerVehicleID(playerid) != SmetlarKamion[1]) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi u smetlarskom kamionu!");
	else if(SmetlarRuta[playerid]>0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Vec imas aktivnu rutu");
	else
	{
	    LociraoVozilo[playerid] = 0;
		SmetlarRuta[playerid] = random(3)+1;
		SmetlarStanica[playerid] = 1;
		SendClientMessage(playerid,-1,""PLAVA"[Smetlar ruta]"BIJELA" Pokupite smece po gradu");
		switch(SmetlarRuta[playerid])
		{
			case 1: SetPlayerCheckpoint(playerid,832.9716,-595.2429,15.9230,5.0);
			case 2: SetPlayerCheckpoint(playerid,688.8415,-628.8930,15.9941,5.0);
			case 3: SetPlayerCheckpoint(playerid,714.0268,-568.2135,15.9941,5.0);
		}
	}
	return 1;
}

CMD:prodajpizzu(playerid,params[])
{
	new id,kolicina,cijena,Float:x,Float:y,Float:z;
	GetPlayerPos(playerid,x,y,z);
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
	else if(PlayerInfo[ playerid ][ pPosao ] != 2) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi prodavac pizze!");
	else if(sscanf(params,"uii",id,kolicina,cijena)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Koristi: /prodajpizzu [id / name] [kolicina] [cijena]");
	else if(!IsPlayerConnected(id)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Pogresan ID korisnika");
	else if(kolicina<1 || kolicina>5) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Kolicina pizze mora biti izmedu 1 i 5");
	else if(cijena<100 || cijena>2000) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Cijena pizze mora biti izmedu 100$ i 2000$");
	else if(!IsPlayerInRangeOfPoint(id,7.0,x,y,z)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Igrac nije u vasoj blizini");
	else if(GetPlayerVehicleID(playerid) != PizzaMotor[0] && GetPlayerVehicleID(playerid) != PizzaMotor[1]) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti na motoru!");
	else if(id == playerid) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Nemozes sam sebi prodati pizzu");
	else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else
	{
		new namex[32],msg[80];
		GetPlayerName(id,namex,sizeof(namex));
		format(msg,sizeof(msg),""NARANCASTA" Ponudio si %s %d komada pizze po cijeni %d$",namex,kolicina,cijena);
		SendClientMessage(playerid,-1,msg);
		GetPlayerName(playerid,namex,sizeof(namex));
		format(msg,sizeof(msg),""NARANCASTA" %s vam je ponudio %d komada pizze po cijeni %d$, pisi /prihvati  da prihvatis",namex,kolicina,cijena);
		SendClientMessage(id,-1,msg);
		PonudioPizzu[id]=playerid;
		PonudenaCijenaPizze[id]=cijena;
		PonudenaKolicinaPizze[id]=kolicina;
	}
	return 1;
}

CMD:kupipizzu(playerid,params[])
{
	new kolicina,cijena;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
	else if(PlayerInfo[ playerid ][ pPosao ] != 2) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nisi prodavac pizze!");
	else if(sscanf(params,"d",kolicina)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Koristi: /kupipizzu [kolicina]");
	else if(kolicina<1 || kolicina>5) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Kolicina pizze mora biti izmedu 1 i 5");
	else if(KolicinaPizze[playerid] > 9) SCM(playerid,-1,""CRVENA"[Greska] "BIJELA"Kod sebe imas puno komada pizze i ne mozes vise kupiti");
	else if((200 * kolicina)>GetPlayerMoney(playerid)) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" Nemas dovoljno novca");
	else if(GetPlayerVehicleID(playerid) != PizzaMotor[0] && GetPlayerVehicleID(playerid) != PizzaMotor[1]) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras biti na motoru!");
	else
	{
		new msg1[100];
		format(msg1,sizeof(msg1),""NARANCASTA"Kupio si %d komada pizze po cijeni %d$",kolicina,cijena);
		SendClientMessage(playerid,-1,msg1);
		KolicinaPizze[playerid]=KolicinaPizze[playerid]+kolicina;
		GivePlayerMoney(playerid,-cijena);
	}
	return 1;
}

CMD:jedipizzu(playerid,params[])
{
	new msg[55],namex[32],Float:hp;
    if(LogedIn[playerid] == 0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Moras se logirati da bi koristio ovu komandu");
    else if(KolicinaPizze[playerid]==0) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemas pizze!");
    else if(UWaru[playerid] == 1) SendClientMessage(playerid,-1, ""CRVENA"[Greska]"BIJELA" U waru si !");
	else if(Prisustvuje[playerid] == true) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Na eventu si !");
	else if(PojeoPizze[playerid]==5) SendClientMessage(playerid,-1,""CRVENA"[Greska]"BIJELA" Nemozes stalno jesti pizzu!");
    else
    {
        KolicinaPizze[playerid]=KolicinaPizze[playerid]-1;
        GetPlayerName(playerid,namex,sizeof(namex));
        format(msg,sizeof(msg),""NARANCASTA"Pojeo si pizzu, kod sebe imas jos %d komada",KolicinaPizze[playerid]);
        SendClientMessage(playerid,-1,msg);
        format(msg, sizeof(msg), ""NARANCASTA" *%s jede pizzu", namex);
		ProxDetector(25.0, playerid, msg, -1,-1,-1,-1,-1);
        GetPlayerHealth(playerid,hp);
        hp = hp+20;
        PojeoPizze[playerid]++;
		SetPlayerHealth(playerid,hp);
		if(!IsPlayerInAnyVehicle(playerid)) ApplyAnimation(playerid,"FOOD","EAT_Pizza",4.1,1,1,1,0,7000,0);
	}
	return 1;
}

CMD:dostavipizzu(playerid,params[])
